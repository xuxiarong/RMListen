<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rm.module_play">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="debug.PlayApplication"
        android:icon="@mipmap/play_ic_launcher"
        android:label="@string/play_app_name"
        android:theme="@style/BusinessAppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".activity.TestActivity"></activity>
        <!-- ui适配设置 -->
        <meta-data
            android:name="design_width_in_dp"
            android:value="375" />
        <meta-data
            android:name="design_height_in_dp"
            android:value="812" />

        <activity android:name=".activity.PlayActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>