<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MineCommonMaterialView">
        <attr name="material_name" format="string" />
        <attr name="user_text" format="string" />
        <attr name="material_icon" format="reference" />
        <attr name="need_line" format="boolean" />
    </declare-styleable>

    <declare-styleable name="MineCommonSettingView">
        <attr name="setting_name" format="string" />
        <attr name="setting_tip" format="string" />
        <attr name="setting_checked" format="boolean" />
        <attr name="setting_show_icon" format="boolean" />
        <attr name="setting_show_switch" format="boolean" />
    </declare-styleable>

    <declare-styleable name="WheelView">
        <attr name="wheelView_gravity">
            <enum name="center" value="17" />
            <enum name="left" value="3" />
            <enum name="right" value="5" />
        </attr>
        <attr name="wheelView_textCenterSize" format="dimension" />
        <attr name="wheelView_textOutSize" format="dimension" />
        <attr name="wheelView_textColorOut" format="color" />
        <attr name="wheelView_textColorCenter" format="color" />
        <attr name="wheelView_dividerColor" format="color" />
        <attr name="wheelView_dividerWidth" format="dimension" />
        <attr name="wheelView_lineSpacingMultiplier" format="float" />
        <attr name="wheelView_label" format="string" />
        <attr name="wheelView_isShowLabel" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CropImageView">
        <attr name="cropMaskColor" format="color" />
        <attr name="cropBorderColor" format="color" />
        <attr name="cropBorderWidth" format="dimension" />
        <attr name="cropFocusWidth" format="dimension" />
        <attr name="cropFocusHeight" format="dimension" />
        <attr name="cropStyle" format="enum">
            <enum name="rectangle" value="0" />
            <enum name="circle" value="1" />
        </attr>
    </declare-styleable>


</resources>