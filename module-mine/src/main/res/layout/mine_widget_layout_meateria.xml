<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/mine_widget_common_material_name"
        style="@style/BusinessTextStyleFirst_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        tools:text="名字" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/mine_widget_common_material_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        android:src="@drawable/business_icon_arrow_db" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/mine_widget_common_material_user_icon"
        android:layout_width="@dimen/dp_52"
        android:layout_height="@dimen/dp_52"
        android:layout_centerVertical="true"
        android:layout_marginEnd="@dimen/dp_8"
        android:layout_toStartOf="@id/mine_widget_common_material_icon"
        android:visibility="invisible" />

    <com.rm.business_lib.wedgit.iamge.RoundImageView
        android:id="@+id/mine_widget_common_material_red_point"
        android:layout_width="@dimen/dp_6"
        android:layout_height="@dimen/dp_6"
        android:layout_centerVertical="true"
        android:layout_marginEnd="@dimen/dp_6"
        android:layout_toStartOf="@id/mine_widget_common_material_icon"
        android:visibility="invisible"
        app:rv_backgroundColor="@color/business_color_ff5e5e"
        app:rv_cornerRadius="@dimen/dp_6" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/mine_widget_common_material_user_text"
        style="@style/BusinessTextStyleThird_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginStart="@dimen/dp_20"
        android:layout_marginEnd="@dimen/dp_6"
        android:layout_toStartOf="@id/mine_widget_common_material_red_point"
        android:layout_toEndOf="@id/mine_widget_common_material_name"
        android:gravity="end"
        android:singleLine="true"
        android:visibility="gone"
        tools:text="123"
        tools:visibility="visible" />
</merge>