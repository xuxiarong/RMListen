<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.rm.module_listen">

    <application
        android:name="debug.ListenApplication"
        android:icon="@mipmap/listen_ic_launcher"
        android:label="@string/listen_app_name"
        android:theme="@style/BusinessAppTheme">
        <!-- ui适配设置-->
        <meta-data
            android:name="design_width_in_dp"
            android:value="375" />
        <meta-data
            android:name="design_height_in_dp"
            android:value="812" />

        <activity android:name="debug.ListenMainDebugActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".player.TestPalyerActivity"></activity>

        <!--已购-->
        <activity
            android:name=".activity.ListenBoughtActivity"
            android:screenOrientation="portrait" />

        <!--订阅-->
        <activity
            android:name=".activity.ListenSubscriptionActivity"
            android:screenOrientation="portrait" />

        <!--听单-->
        <activity
            android:name=".activity.ListenSheetListActivity"
            android:screenOrientation="portrait" />

        <!--听单详情-->
        <activity
            android:name=".activity.ListenMySheetDetailActivity"
            android:screenOrientation="portrait" />
    </application>
</manifest>