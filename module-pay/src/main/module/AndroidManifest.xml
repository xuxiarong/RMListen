<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rm.module_play">

    <application
        android:name="debug.PayApplication"
        android:icon="@mipmap/pay_ic_launcher"
        android:label="@string/pay_app_name"
        android:theme="@style/BusinessAppTheme">
        <!-- ui适配设置-->
        <meta-data
            android:name="design_width_in_dp"
            android:value="375" />
        <meta-data
            android:name="design_height_in_dp"
            android:value="812" />

        <activity android:name="debug.PayMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>